<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form"
	controllerName="stipAdmin.stipAdmin.controller.displayGruppi_Scheda" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title">
				<customHeader>
					<Bar>
						<contentMiddle>
							<Title text="Display Gruppo Scheda"/>
						</contentMiddle>
						<contentRight>
							<Text id="fy"/>
						</contentRight>
					</Bar>
				</customHeader>
				<content>
					<f:SimpleForm layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false"
						emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1">
						<Label text="Id Gruppo Scheda"/>
						<Input value="{displayGruppiSchedaModel>/0/ID}" editable="false"/>
						<Label text="Descrizione"/>
						<Input value="{displayGruppiSchedaModel>/0/DESC}" editable="false"/>
						<Label text="Template Lettere"/>
						<Input value="{displayGruppiSchedaModel>/0/TEMPLATE_LETTERE}" editable="false"/>
					</f:SimpleForm>
				</content>
				<Toolbar>
					<ToolbarSpacer/>
					<Button xmlns="sap.m" width="200px" text="Annulla" id="button1" press="cancel"/>
					<ToolbarSpacer/>
				</Toolbar>
			</Page>
		</pages>
	</App>
</mvc:View>